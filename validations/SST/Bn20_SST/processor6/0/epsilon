FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-20;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    wall
    {
        type            fixedValue;
        value           uniform 1e-20;
    }
    procBoundary6to0
    {
        type            processor;
        value           uniform 1e-20;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 1e-20;
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 1e-20;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 1e-20;
    }
}


