FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    wall
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 1000;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1000;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 1000;
    }
}


